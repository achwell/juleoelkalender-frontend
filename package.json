{
    "name": "frontend",
    "version": "1.1.26-SNAPSHOT",
    "private": true,
    "homepage": "https://juleoelkalender.no",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
        "format": "prettier --write src/**/**/**/**/**/**/*.{ts,tsx,css} __tests__/**/*.{ts,tsx} openapi*.*",
        "lint:fix": "eslint src --report-unused-disable-directives --max-warnings 0 --fix",
        "preview": "vite preview",
        "test": "vitest run",
        "test:ui": "vitest --ui",
        "coverage": "vitest run --coverage",
        "generate-types": "openapi-ts",
        "postinstall": "npm run generate-types",
        "sonar": "sonar-scanner",
        "prepare": "husky"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@hookform/resolvers": "^5.2.2",
        "@react-oauth/google": "^0.13.4",
        "@reduxjs/toolkit": "^2.11.2",
        "@shaqrivera/email-validator": "^2.0.4",
        "@tailwindcss/vite": "^4.1.18",
        "@tanstack/match-sorter-utils": "^8.19.4",
        "@tanstack/react-table": "^8.21.3",
        "chart.js": "^4.5.1",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "date-fns-tz": "^3.2.0",
        "husky": "^9.1.7",
        "i18next": "^25.7.3",
        "i18next-browser-languagedetector": "^8.2.0",
        "i18next-http-backend": "^3.0.2",
        "jwt-decode": "^4.0.0",
        "react": "^19.2.3",
        "react-chartjs-2": "^5.3.1",
        "react-day-picker": "^9.13.0",
        "react-device-detect": "^2.2.3",
        "react-dom": "^19.2.3",
        "react-error-boundary": "^6.0.1",
        "react-flags-select": "^2.5.0",
        "react-hook-form": "^7.69.0",
        "react-i18next": "^16.5.1",
        "react-icons": "^5.5.0",
        "react-redux": "^9.2.0",
        "react-router": "^7.11.0",
        "react-social-login-buttons": "^4.1.1",
        "react-toastify": "^11.0.5",
        "reactjs-social-login": "^2.6.3",
        "redux-persist": "^6.0.0",
        "uuid": "^13.0.0",
        "zod": "^4.3.4"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@hey-api/openapi-ts": "^0.89.2",
        "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.1",
        "@types/node": "^22.19.3",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@types/react-test-renderer": "^19.1.0",
        "@vitejs/plugin-react-swc": "^4.2.2",
        "@vitest/coverage-v8": "^4.0.16",
        "@vitest/ui": "^4.0.16",
        "eslint": "^9.39.2",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.26",
        "happy-dom": "^20.0.11",
        "prettier": "^3.7.4",
        "sass": "^1.97.1",
        "sonar-scanner": "^3.1.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.51.0",
        "vite": "^7.3.0",
        "vite-plugin-pwa": "^1.2.0",
        "vite-tsconfig-paths": "^6.0.3",
        "vitest": "^4.0.16"
    },
    "lint-staged": {
        "**/*.{ts,tsx,scss}": [
            "prettier --write"
        ]
    },
    "prettier": {
        "singleQuote": false,
        "semi": true,
        "plugins": [
            "@ianvs/prettier-plugin-sort-imports"
        ],
        "importOrder": [
            "^react$",
            "^react-dom/client$",
            "^react(.*)$",
            "^@reduxjs/(.*)$",
            "redux-persist",
            "^redux(.*)$",
            "@redux/(.*)$",
            "^@components/(.*)$",
            "^@pages/(.*)$",
            "^@types/(.*)$",
            "^[./](.*)$",
            "^(.*)/(.*)(.css)$",
            "^(.*)/(.*)(.scss)$"
        ],
        "importOrderParserPlugins": [
            "typescript",
            "jsx",
            "decorators-legacy"
        ],
        "importOrderTypeScriptVersion": "5.0.0",
        "importOrderCaseSensitive": true,
        "trailingComma": "es5",
        "tabWidth": 4,
        "printWidth": 120,
        "bracketSpacing": true
    },
    "resolutions": {
        "react-dev-utils": "12.0.1"
    },
    "overrides": {
        "react": "^19.2.3",
        "react-dom": "^19.2.3"
    },
    "engines": {
        "node": ">=22"
    }
}
